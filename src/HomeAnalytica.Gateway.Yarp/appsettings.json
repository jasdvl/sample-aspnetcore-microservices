{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ReverseProxy": {
        "Routes": {
            "dataCollection": {
                "ClusterId": "dataCluster",
                "Match": {
                    "Path": "/data/{**catch-all}"
                }
            }
        },
        "Clusters": {
            "dataCluster": {
                "Destinations": {
                    "destination1": {
                        // Data Collection Service
                        "Address": "https://localhost:5220"
                    }
                }
            }
        }
    },
    "AllowedHosts": "*"
}
